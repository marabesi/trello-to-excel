{
    "manifest_version": 2,
    "name": "Trello board to excel",
    "description": "Simple extension to export your trello board to excel",
    "version": "0.1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "Trello to excel!"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "permissions": [
        "activeTab",
        "downloads"
    ]
}